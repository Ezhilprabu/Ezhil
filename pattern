public class JavaProgram
{
    public static void main(String args[])
    {
        int i, j;
        
        for(i=1; i<=55; i++){
        if (i%2==0)
        {
            for(j=0; j<3; j++)
            {
                System.out.print("* ");
            }
            System.out.println();
        }
        else
        {
        for(j=0; j<5; j++)
            {
                System.out.print("* ");
            }
            System.out.println();
        }
        }
    }
}
